<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
    
    <bean id="ans1" class="answer.Answer">
        <property name="id" value="10"></property>
        <property name="answer" value="java is programming laguage"></property>
        <property name="postedby" value="shubham"></property>
    </bean>
    
    <bean id="ans2" class="answer.Answer">
        <property name="id" value="11"></property>
        <property name="answer" value="java is robust"></property>
        <property name="postedby" value="sanket"></property>
    </bean>
    
    <bean id="user1" class="answer.User">
        <constructor-arg value="100"></constructor-arg>
        <constructor-arg value="shubham@gmail.com"></constructor-arg>
    </bean>
    
    <bean id="user2" class="answer.User">
        <constructor-arg value="101"></constructor-arg>
        <constructor-arg value="sanket@gmail.com"></constructor-arg>
    </bean>
    
    <bean id="quesNonStringMap" class="question.QuestionNonStringMap">
        <property name="id" value="1001"></property>
        <property name="question" value="what is java"></property>
        <property name="answers">
            <map>
                <entry key-ref="ans1" value-ref="user1"></entry>
                <entry key-ref="ans2" value-ref="user2"></entry>
            </map>
        </property>
    </bean>
</beans>
